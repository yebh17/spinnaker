pipeline {
    environment {
    registry = "python-app"
    dockerImage = '' 
    }
    agent any
    stages { 
        stage('Build') {
            steps {
                dockerImage = docker.build registry
            }
        }

        stage('check'){
            steps {
                sh 'docker images'
            }
        }
    }

    post {
        always {
            cleanWs()
        }
    }
}